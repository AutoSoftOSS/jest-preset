{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "source/**/*.ts"
  ],
  "coverageProvider": "v8",
  "coverageReporters": [
    "text"
  ],
  "moduleNameMapper": {
    "^#(.*)": "$1",
    "^(\\.{1,2}/.*)\\.js$": "$1"
  },
  "testRegex": "test/(.*).test.ts$",
  "transform": {
    "^.+\\.tsx?$": [
      "@swc/jest"
    ]
  }
}